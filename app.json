{
  "expo": {
    "name": "modern-chat-video",
    "slug": "modern-chat-video",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "modernchatvideo",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibrazaou.modernchatvideo",
      "associatedDomains": [
        "applinks:known-sunfish-56.clerk.accounts.dev",
        "webcredentials:known-sunfish-56.clerk.accounts.dev"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ibrazaou.modernchatvideo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [ "expo-build-properties", {
          "ios": {
            "deploymentTarget": "16.0"
          }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5f210c8f-77cc-41c3-81d0-51978996609d"
      }
    }
  }
}
